TARGETNAME=ndascmd
TARGETTYPE=PROGRAM
TARGETPATH=obj

BUILD_CONSUMES=ndasuser

C_DEFINES=/DUNICODE /D_UNICODE

INCLUDES=.\inc;$(NDAS_INC_PATH);$(NDAS_INC_PATH)\ndas;$(PSDK_INC_PATH)
RCOPTIONS=/i $(NDAS_INC_PATH) /i $(NDAS_INC_PATH)\ndas
UMENTRY=wmain
UMTYPE=console
USER_C_FLAGS=/Wp64

!IF $(FREEBUILD)
USE_MSVCRT=1
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

TARGETLIBS=\
	$(PSDK_LIB_PATH)\kernel32.lib \
	$(PSDK_LIB_PATH)\user32.lib \
    $(PSDK_LIB_PATH)\advapi32.lib \
	$(PSDK_LIB_PATH)\shlwapi.lib \
    $(PSDK_LIB_PATH)\shell32.lib \
	$(NDAS_LIB_PATH)\ndasuser.lib

#DELAYLOAD_SUPPORTED=1
#DELAYLOAD=ndasuser.dll
#DELAYLOAD_UNLOAD_SUPPORT=1

BROWSER_INFO=1
BROWSERFILE=$(TARGETNAME).bsc -n

MSC_WARNING_LEVEL=-W3 -WX
#MSC_OPTIMIZATION=/Oitb2
#MSC_OPTIMIZATION=/Od /Oi

PRECOMPILED_INCLUDE=stdafx.h

SOURCES= ndascmd.c
