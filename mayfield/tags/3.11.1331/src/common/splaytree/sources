TARGETNAME=libsplaytree
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=libsplaytree

# to publish headers
NTTARGETFILE1=$(NDAS_INC_PATH)\top-down-splay.h

PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

MSC_WARNING_LEVEL=-W3 -WX
!IF $(FREEBUILD)
MSC_OPTIMIZATION=/Ox
!ELSE
!ENDIF

SOURCES=top-down-splay.c
