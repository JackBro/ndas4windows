UMTYPE=console

!IFDEF USE_UNICODE
C_DEFINES=-DUNICODE -D_UNICODE
UMENTRY=wmain
!ELSE
UMENTRY=main
!ENDIF

LINK_TIME_CODE_GENERATION=1

INCLUDES=$(NDAS_INC_PATH)

!IF $(FREEBUILD)
#USE_MSVCRT=1
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

TARGETLIBS=\
	$(NDAS_LIB_PATH)\des.lib \
	$(NDAS_LIB_PATH)\scrc32.lib \
	$(NDAS_LIB_PATH)\ndasid.lib \
	$(NDAS_LIB_PATH)\ndasidenc.lib

SOURCES=main.c

