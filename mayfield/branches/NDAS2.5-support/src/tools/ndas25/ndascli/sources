TARGETNAME=ndascli
TARGETTYPE=PROGRAM
TARGETPATH=obj

INCLUDES=$(NDAS_INC_PATH);$(PSDK_INC_PATH);$(NDAS_EXTERN_INC_PATH)

MSC_WARNING_LEVEL=-W3 -WX

UMTYPE=console
UMENTRY=main

C_DEFINES=/DUNICODE /D_UNICODE
USECXX_FLAG=/Tp
USER_C_FLAGS=/Wp64
USE_NATIVE_EH=1
USE_STL=1

!IF $(FREEBUILD)
USE_MSVCRT=1
MSC_OPTIMIZATION=/Ox
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

BUILD_CONSUMES=aes

TARGETLIBS= \
	$(PSDK_LIB_PATH)\user32.lib \
	$(PSDK_LIB_PATH)\ws2_32.lib \
	$(NDAS_LIB_PATH)\aes.lib

SOURCES=\
	LanScsiCli.cpp \
	encdec.cpp \
	misccmds.cpp \
	patterns.cpp \
	smart.cpp \
	meta.cpp\
	scrc32.cpp 
	