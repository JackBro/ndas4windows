TARGETNAME=apitest
TARGETTYPE=PROGRAM
TARGETPATH=obj

INCLUDES=$(NDAS_PUBLIC_INC_PATH);$(NDAS_INC_PATH);$(PSDK_INC_PATH)

UMTYPE=console

C_DEFINES=/DUNICODE /D_UNICODE

MSC_WARNING_LEVEL=-W3 -WX
!IF $(FREEBUILD)
USE_MSVCRT=1
MSC_OPTIMIZATION=/Ox
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

BUILD_CONSUMES=ndasop_dll

TARGETLIBS= \
	$(SDK_LIB_PATH)\user32.lib \
	$(NDAS_PUBLIC_LIB_PATH)\ndasop.lib
	
SOURCES=apitest.c
