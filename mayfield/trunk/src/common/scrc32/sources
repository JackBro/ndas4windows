TARGETNAME=libscrc32
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=libscrc32

# to publish headers
NTTARGETFILE1=$(NDAS_INC_PATH)\scrc32.h

PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}
MSC_WARNING_LEVEL=-W3 -WX

!IF $(FREEBUILD)
MSC_OPTIMIZATION=/Ox
!ELSE
!ENDIF

SOURCES=scrc32.c
