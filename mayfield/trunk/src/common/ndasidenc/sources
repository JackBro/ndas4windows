TARGETNAME=libndasidenc
TARGETTYPE=LIBRARY
TARGETPATH=obj

BUILD_PRODUCES=libndasidenc

C_DEFINES=
INCLUDES=$(NDAS_INC_PATH);$(NDAS_SRC_PATH)\inc\public;$(INCLUDES)

# to publish headers
NTTARGETFILE1=$(NDAS_INC_PATH)\ndas\ndasidenc.h

!IF $(FREEBUILD)
MSC_OPTIMIZATION=/Ox
!ELSE
!ENDIF

# to publish library
PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

USECXX_FLAG=/TC

MSC_WARNING_LEVEL=-W3 -WX -Wp64

SOURCES=ndasidenc.c
