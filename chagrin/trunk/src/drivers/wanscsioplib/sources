TARGETNAME=libwanscsioplib
TARGETTYPE=LIBRARY
TARGETPATH=obj

INCLUDES=$(NDAS_INC_PATH)
RCOPTIONS=/i $(NDAS_INC_PATH)

!IF $(FREEBUILD)
USE_MSVCRT=1
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

# publish lib files to the project root
PASS1_PUBLISH= {$(O)\$(TARGETNAME).lib=$(NDAS_LIB_PATH)\$(TARGETNAME).lib}

SOURCES=wanscsioplib.c
