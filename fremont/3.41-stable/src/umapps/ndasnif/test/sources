TARGETNAME=ndasnif
TARGETTYPE=PROGRAM
TARGETPATH=obj

C_DEFINES=/DUNICODE /D_UNICODE

INCLUDES=..;..\inc
UMTYPE=console
UMENTRY=wmain

!IF $(FREEBUILD)
USE_MSVCRT=1
MSC_OPTIMIZATION=/Ox
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

INCLUDES=$(PSDK_INC_PATH);$(NDAS_PUBLIC_INC_PATH)

SOURCES=main.cpp 

BUILD_CONSUMES=ndasnif

TARGETLIBS=\
	$(PSDK_LIB_PATH)\kernel32.lib \
	$(PSDK_LIB_PATH)\ole32.lib \
	..\dll\$(O)\ndasnif.lib

