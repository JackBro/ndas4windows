TARGETNAME=autorun
TARGETPATH=obj
TARGETTYPE=PROGRAM

C_DEFINES=$(C_DEFINES) -DUNICODE -D_UNICODE
UMTYPE=windows
UMENTRY=wwinmain

USE_ATL=1
ATL_VER=70

PRECOMPILED_INCLUDE=stdafx.h
PRECOMPILED_CXX=1

INCLUDES=$(PSDK_INC_PATH);$(O)

!IF $(FREEBUILD)
USE_MSVCRT=1
MSC_OPTIMIZATION=/Ox
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

# WDK 6000 supports USE_OBJECT_ROOT
USE_OBJECT_ROOT=1
# WINDDK 3790.1830 requires PASS0_HEADERDIR, PASS0_SOURCEDIR
PASS0_HEADERDIR=$(O)
PASS0_SOURCEDIR=$(O)

SOURCES=\
	AboutDlg.cpp \
	AutoRun.cpp \
	AutoRunCom.cpp \
	AutoRunView.cpp \
	AutoRunHelper.idl \
	MainFrm.cpp \
	AutoRun.rc

TARGETLIBS=\
	$(PSDK_LIB_PATH)\oleaut32.lib \
	$(PSDK_LIB_PATH)\ole32.lib \
	$(PSDK_LIB_PATH)\shlwapi.lib \
	$(PSDK_LIB_PATH)\shell32.lib \
	$(PSDK_LIB_PATH)\comctl32.lib \
	$(PSDK_LIB_PATH)\uuid.lib

!INCLUDE $(WTL_SOURCES_INC)
