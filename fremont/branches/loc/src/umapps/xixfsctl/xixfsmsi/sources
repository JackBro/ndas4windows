TARGETNAME=xixfsmsi
TARGETTYPE=DYNLINK
TARGETPATH=obj

!IF $(FREEBUILD)
USE_MSVCRT=1
MSC_OPTIMIZATION=/Ox
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

C_DEFINES=/DUNICODE /D_UNICODE
INCLUDES=$(NDAS_INC_PATH);$(PSDK_INC_PATH)
RCOPTIONS=/i $(NDAS_SDK_INC_PATH) 

DLLENTRY=_DllMainCRTStartup
DLLDEF=$(O)\$(TARGETNAME).def

TARGETLIBS= \
	$(SDK_LIB_PATH)\kernel32.lib \
	$(SDK_LIB_PATH)\advapi32.lib \
	$(NDAS_LIB_PATH)\libxixfsctl.lib

SOURCES= \
	xixfsmsi.cpp \
	xixfsmsi.rc

