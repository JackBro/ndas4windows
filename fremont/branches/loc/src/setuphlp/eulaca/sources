TARGETNAME=eulaca
TARGETTYPE=DYNLINK
TARGETPATH=obj

INCLUDES=$(PSDK_INC_PATH);$(NDAS_INC_PATH)
RCOPTIONS=/I$(NDAS_INC_PATH)

UMTYPE=console
DLLDEF=$(O)\$(TARGETNAME).def
DLLENTRY=_DllMainCRTStartup

C_DEFINES=/DUNICODE /D_UNICODE

!IF $(FREEBUILD)
USE_LIBCMT=1
USE_MSVCRT=1
#MSC_OPTIMIZATION=/Osi1
MSC_OPTIMIZATION=/Ox
!ELSE
USE_LIBCMT=1
DEBUG_CRTS=1
!ENDIF

VC7_SYMBOLS=1

TARGETLIBS= \
	$(PSDK_LIB_PATH)\kernel32.lib \
	$(PSDK_LIB_PATH)\msi.lib

SOURCES= \
    eulaca.cpp \
    msisup.cpp \
	dllentry.cpp \
    eulaca.rc
